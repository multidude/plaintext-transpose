{{range .LedgerData}}
{{.Datetime}} * {{.Comment1 }}
;{{.Comment2 }}
{{- if eq .Type "Deposit"}}
	Assets:Electrum			{{.Amount}} BTC
	Income:Ambiguous
{{- end}}
{{- if eq .Type "Withdrawal"}}
	Assets:Electrum			{{.Amount}} BTC
		{{- if .Fee}}
	Expenses:Fees:OnChain		{{.Fee}} BTC
		{{- end}}
	Expenses:Ambiguous
{{- end}}
{{end}}
